-- Scripts

local function QBBVR_fake_script() -- KeySystemFrame.LocalScript 
	local script = Instance.new('LocalScript', KeySystemFrame)

	local UserInputService = game:GetService("UserInputService")
	local frame = script.Parent -- KeySystemFrame
	local dragToggle = false
	local dragSpeed = 0.25 -- Adjust as needed
	local dragStart = nil
	local startPos = nil
	
	-- Function to update the frame position
	local function updateInput(input)
		local delta = input.Position - dragStart
		local newPos = UDim2.new(
			startPos.X.Scale, 
			startPos.X.Offset + delta.X, 
			startPos.Y.Scale, 
			startPos.Y.Offset + delta.Y
		)
		frame.Position = newPos
	end
	
	-- Detect when the mouse button is pressed on the frame
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	-- Update the frame position while dragging
	UserInputService.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement and dragToggle then
			updateInput(input)
		end
	end)
end
coroutine.wrap(QBBVR_fake_script)()
local function MQYBMD_fake_script() -- KeySystemFrame.LocalScript 
	local script = Instance.new('LocalScript', KeySystemFrame)

	local HttpService = game:GetService("HttpService")
	local StarterGui = game:GetService("StarterGui")
	local KeyBox = script.Parent.KeyFrame.KeyBox
	local CheckKeyButton = script.Parent.KeyFrame["CheckKey Button"]
	local DiscordButton = script.Parent.KeyFrame["Discord Button"]
	local KeySystemFrame = script.Parent
	local AngelBronx = script.Parent.Parent:FindFirstChild("AngelBronx")
	
	-- Keys URL
	local KEYS_URL = "https://raw.githubusercontent.com/solarDev2/Key-System/refs/heads/main/keydiddyweowykeys"
	
	-- Cache for keys to avoid multiple HTTP requests
	local validKeys = {}
	local keysLoaded = false
	
	-- Improved key fetching with error handling
	local function loadKeys()
		if keysLoaded then return end
	
		local success, result = pcall(function()
			local rawKeys = HttpService:GetAsync(KEYS_URL, true)
			-- Split by lines and trim each line
			for line in rawKeys:gmatch("[^\r\n]+") do
				local cleanKey = line:match("^%s*(.-)%s*$") -- Trim whitespace
				if #cleanKey > 0 then -- Only add non-empty keys
					table.insert(validKeys, cleanKey)
				end
			end
			keysLoaded = true
			print("Successfully loaded", #validKeys, "keys")
		end)
	
		if not success then
			warn("Failed to load keys:", result)
			-- You could add fallback keys here if needed
			table.insert(validKeys, "DEFAULT_KEY_123") -- Example fallback
		end
	end
	
	-- Case-insensitive key check with trimming
	local function isValidKey(inputKey)
		if not keysLoaded then loadKeys() end
	
		local userKey = inputKey:match("^%s*(.-)%s*$"):lower() -- Trim and lowercase
	
		for _, validKey in ipairs(validKeys) do
			if validKey:lower() == userKey then
				return true
			end
		end
	
		return false
	end
	
	-- Notification function
	local function showSuccessNotification()
		StarterGui:SetCore("SendNotification", {
			Title = "ACCESS GRANTED",
			Text = "Welcome to Angel Bronx!",
			Icon = "rbxassetid://78000615686315",
			Duration = 5,
		})
	end
	
	-- Grant access function
	local function grantAccess()
		showSuccessNotification()
		KeySystemFrame.Visible = false
		if AngelBronx then
			AngelBronx.Visible = true
		else
			warn("AngelBronx frame not found in parent")
		end
	end
	
	-- Check Key button
	CheckKeyButton.MouseButton1Click:Connect(function()
		local key = KeyBox.Text
		if isValidKey(key) then
			grantAccess()
		else
			KeyBox.Text = ""
			KeyBox.PlaceholderText = "Invalid Key! Try Again."
			-- Optional: Play error sound
		end
	end)
	
	-- Discord button
	if DiscordButton then
		DiscordButton.MouseButton1Click:Connect(function()
			local discordInvite = "https://discord.gg/yourlink"
			pcall(function()
				StarterGui:SetCore("OpenBrowserWindow", {
					URL = discordInvite
				})
			end)
		end)
	end
	
	-- Load keys when the frame first appears
	loadKeys()
end
coroutine.wrap(MQYBMD_fake_script)()
