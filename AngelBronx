-- Scripts

local function OZWVIXY_fake_script() -- KeySystemFrame.LocalScript 
	local script = Instance.new('LocalScript', KeySystemFrame)

	local UserInputService = game:GetService("UserInputService")
	local frame = script.Parent -- KeySystemFrame
	local dragToggle = false
	local dragSpeed = 0.25 -- Adjust as needed
	local dragStart = nil
	local startPos = nil
	
	-- Function to update the frame position
	local function updateInput(input)
		local delta = input.Position - dragStart
		local newPos = UDim2.new(
			startPos.X.Scale, 
			startPos.X.Offset + delta.X, 
			startPos.Y.Scale, 
			startPos.Y.Offset + delta.Y
		)
		frame.Position = newPos
	end
	
	-- Detect when the mouse button is pressed on the frame
	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	-- Update the frame position while dragging
	UserInputService.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement and dragToggle then
			updateInput(input)
		end
	end)
end
coroutine.wrap(OZWVIXY_fake_script)()
local function CFZGE_fake_script() -- KeySystemFrame.LocalScript 
	local script = Instance.new('LocalScript', KeySystemFrame)

	local HttpService = game:GetService("HttpService")
	local StarterGui = game:GetService("StarterGui")
	local KeyBox = script.Parent.KeyFrame.KeyBox
	local CheckKeyButton = script.Parent.KeyFrame["CheckKey Button"] -- Fixed name
	local DiscordButton = script.Parent.KeyFrame["Discord Button"] -- Fixed name
	local KeySystemFrame = script.Parent
	local AngelBronx = script.Parent.Parent:FindFirstChild("AngelBronx")
	
	-- Keys URL
	local KEYS_URL = "https://raw.githubusercontent.com/solarDev2/Key-System/refs/heads/main/keydiddyweowykeys"
	
	-- Fetch keys from GitHub
	local function fetchKeys()
		local success, keys = pcall(function()
			return HttpService:GetAsync(KEYS_URL, true)
		end)
	
		if success then
			local keyList = {}
			for line in keys:gmatch("[^\r\n]+") do
				table.insert(keyList, line:gsub("%s+", ""))
			end
			return keyList
		else
			warn("Failed to fetch keys: " .. keys)
			return {}
		end
	end
	
	-- Check if the entered key is valid
	local function isValidKey(inputKey)
		local keys = fetchKeys()
		for _, key in pairs(keys) do
			if key == inputKey then
				return true
			end
		end
		return false
	end
	
	-- Show a notification with an image
	local function showSuccessNotification()
		StarterGui:SetCore("SendNotification", {
			Title = "ACCESS GRANTED",
			Text = "Welcome to Angel Bronx!",
			Icon = "rbxassetid://78000615686315", -- Your image ID
			Duration = 5, -- Notification duration (seconds)
		})
	end
	
	-- Remove the key system and show AngelBronx
	local function grantAccess()
		showSuccessNotification() -- Show notification first
		KeySystemFrame.Visible = false
		if AngelBronx then
			AngelBronx.Visible = true
		else
			warn("AngelBronx frame not found!")
		end
	end
	
	-- Check Key button logic
	CheckKeyButton.MouseButton1Click:Connect(function()
		local key = KeyBox.Text:gsub("%s+", "") -- Remove spaces
		if isValidKey(key) then
			grantAccess()
		else
			KeyBox.Text = ""
			KeyBox.PlaceholderText = "Invalid Key! Try Again."
		end
	end)
	
	-- Discord button logic (optional)
	if DiscordButton then
		DiscordButton.MouseButton1Click:Connect(function()
			local discordInvite = "https://discord.gg/yourlink" -- Replace with your link
			print("Join Discord: " .. discordInvite)
			pcall(function()
				StarterGui:SetCore("OpenBrowserWindow", {
					URL = discordInvite
				})
			end)
		end)
	end
end
coroutine.wrap(CFZGE_fake_script)()
